{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Faseeh Documentation",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "css": "/custom.css",
  "navigation": {
    "tabs": [
      {
        "tab": "Docs",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart",
              "models"
            ]
          },
          {
            "group": "Capabilities",
            "pages": [
              "capabilities/text-to-speech",
              "capabilities/speech-to-text",
              "capabilities/voice-cloning",
              "capabilities/voice-isolation"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "API documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoints",
            "pages": [
              "api-reference/models",
              "api-reference/text-to-speech-post",
              "api-reference/text-to-speech-stream",
              "api-reference/websocket"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Dashboard",
          "href": "https://app.faseeh.ai",
          "icon": "chart-line"
        },
        {
          "anchor": "Support",
          "href": "mailto:support@faseeh.com",
          "icon": "envelope"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Dashboard",
        "href": "https://app.faseeh.ai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get API Key",
      "href": "https://app.faseeh.ai/en/api-keys"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/faseeh",
      "github": "https://github.com/faseeh",
      "linkedin": "https://linkedin.com/company/faseeh"
    }
  }
}